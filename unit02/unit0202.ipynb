    "from pymongo import MongoClient\n",
    "\n",
    "# 建立連線\n",
    "client = MongoClient(\"mongodb://admin:mymdb$1234@localhost:27017/\")\n",
    "\n",
    "# 列出所有資料庫\n",
    "databases = client.list_database_names()\n",
    "\n",
    "# 輸出結果\n",
    "print(\"Available databases:\")\n",
    "for db in databases:\n",
    "    print(f\"- {db}\")\n"
   ]
